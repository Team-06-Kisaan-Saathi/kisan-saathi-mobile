{
  "expo": {
    "name": "digital-marketplace",
    "slug": "digital-marketplace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "digitalmarketplace",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
  "config": {
    "googleMaps": {
      "apiKey": "AIzaSyAVXLK60fTLjxMWpvNFEtKX6xDt5QooJWo"
    }
  },
  "permissions": [
    "ACCESS_FINE_LOCATION",
    "ACCESS_COARSE_LOCATION",
    "RECORD_AUDIO"
  ],
  "adaptiveIcon": {
    "backgroundColor": "#E6F4FE",
    "foregroundImage": "./assets/images/android-icon-foreground.png",
    "backgroundImage": "./assets/images/android-icon-background.png",
    "monochromeImage": "./assets/images/android-icon-monochrome.png"
  },
  "edgeToEdgeEnabled": true,
  "predictiveBackGestureEnabled": false,
  "package": "com.anonymous.digitalmarketplace"
},
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "react-native-vosk",
        {
          "models": [
            "assets/vosk-model-small-en-in-0.4",
            "assets/vosk-model-small-hi-0.22",
            "assets/vosk-model-small-te-0.42"
          ],
          "iOSMicrophonePermission": "Allow microphone for voice navigation"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "ndkVersion": "25.1.8937393"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "71d1a27d-9242-4bbe-b1e2-424e149ead5a"
      }
    },
    "owner": "unni2005"
  }
}
